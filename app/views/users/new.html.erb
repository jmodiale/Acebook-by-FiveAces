<div>
    <h1>Sign Up</h1>
      <%= bootstrap_form_for @user do |f| %>
        <%= f.text_field :name,label: "Name" %>
        <%= f.text_field :surname,label: "Surname" %>
        <%= f.text_field :username,label: "Username" %>
        <%= f.email_field :email,label: "Email" %>
        <%= f.password_field :password,label: "Password" %>
        <%= f.submit "Submit" %>
      <% end %>
</div>